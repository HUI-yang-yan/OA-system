<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.oa.mapper.EquipmentMapper">


    <select id="getAllEquipmentNum" resultType="java.lang.Integer">
        select count(*) from oa_management_system.equipment;
    </select>
    <select id="getValidEquipmentNum" resultType="java.lang.Integer">
        select count(*) from oa_management_system.equipment where status = 'available';
    </select>
</mapper>
